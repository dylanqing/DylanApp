<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:titleView="http://schemas.android.com/apk/res-auto"
    xmlns:loadinglayout="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.worldunion.dylanapp.widget.title.TitleView
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        titleView:centerDesc="@string/title_msg"
        titleView:leftImage="@drawable/icon_back"/>

    <com.worldunion.dylanapp.widget.LoadingLayout
        android:id="@+id/loading_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:animateLayoutChanges="true"
        loadinglayout:emptyImg="@drawable/loading_layout_empty"
        loadinglayout:emptyTxt="@string/common_empty">






    </com.worldunion.dylanapp.widget.LoadingLayout>



</LinearLayout>